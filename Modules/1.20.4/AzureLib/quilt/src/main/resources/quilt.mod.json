{
  "schema_version": 1,
  "quilt_loader": {
    "group": "${group}",
    "id": "vrglabs_azurelib",
    "version": "${mod_version}",
    "metadata": {
      "name": "Vrglabs AzureLib",
      "description": "Utility classes for AzureLib for using with VrglabsLib",
      "contributors": {
        "Vrglab": "Author"
      },
      "icon": "logo.png"
    },
    "intermediate_mappings": "net.fabricmc:intermediary",
    "entrypoints": {
      "init": [
        "org.Vrglab.AzureLib.quilt.VlAzureLibModQuilt"
      ],
      "client": [
        "mod.azure.azurelib.fabric.ClientListener"
      ]
    },
    "depends": [
      {
        "id": "quilt_loader",
        "version": "*"
      },
      {
        "id": "quilt_base",
        "version": "*"
      },
      {
        "id": "minecraft",
        "version": ">=1.20.4"
      },
      {
        "id": "architectury",
        "version": ">=11.1.17"
      }
    ]
  },
  "mixin": [
    "vrglabs_azurelib.mixins.json"
  ],
  "accessWidener": "vrglabs_azurelib.aw"
}
