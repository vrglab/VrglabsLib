{
  "schemaVersion": 1,
  "id": "vrglabslib",
  "version": "${version}",
  "name": "Vrglabs Lib",
  "description": "Code dependency for all mods to use, make mod making simpler and more dynamic",
  "authors": [
    "Vrglab"
  ],
  "contact": {
    "homepage": "https://fabricmc.net/",
    "sources": "https://github.com/FabricMC/fabric-example-mod"
  },
  "license": "CC0-1.0",
  "icon": "assets/vrglabslib/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "org.Vrglab.fabric.VLModFabric"
    ],
    "client": [
      "org.Vrglab.fabric.client.ExampleModFabricClient"
    ],
    "fabric-datagen": [
      "org.Vrglab.fabric.VLDataGenerator"
    ]
  },
  "mixins": [
    "vrglabslib.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.15.11",
    "minecraft": "~1.20.4",
    "java": ">=17",
    "architectury": ">=11.1.17",
    "fabric-api": "*"
  },
  "suggests": {
    "another-mod": "*"
  }
}
